<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.innovaee.eorder.bean">

	<class name="Category" table="T_CATEGORY">
		<id name="categoryId" column="CATEGORY_ID" type="integer">
			<generator class="increment" />
		</id>
		<property name="categoryName" column="category_NAME" type="string" />
		<property name="categoryPicture" column="category_picture" type="string" />
		<property name="createAt" column="create_at" type="timestamp" />
		<property name="updateAt" column="update_at" type="timestamp" />
	</class>

</hibernate-mapping> 